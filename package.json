{
  "name": "intercal",
  "displayName": "Intercal",
  "description": "Syntax highlighting for Intercal",
  "version": "0.2.0",
  "publisher": "JulianBarberaLabs",
  "repository": {
    "type": "git",
    "url": "https://github.com/JulianBarberaLabs/intercal"
  },
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "intercal",
        "aliases": [
          "Intercal",
          "intercal"
        ],
        "extensions": [
          ".i",
          ".3i",
          ".4i",
          ".5i",
          ".6i",
          ".7i"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "intercal",
        "scopeName": "source.intercal",
        "path": "./syntaxes/intercal.tmLanguage.json"
      }
    ]
  }
}
