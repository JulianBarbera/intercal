{
  "name": "intercal",
  "displayName": "Intercal",
  "description": "Syntax highlighting for Intercal",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "intercal",
      "aliases": ["Intercal", "intercal"],
      "extensions": [".i",".3i",".4i",".5i",".6i",".7i"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "intercal",
      "scopeName": "source.intercal",
      "path": "./syntaxes/intercal.tmLanguage.json"
    }]
  }
}
